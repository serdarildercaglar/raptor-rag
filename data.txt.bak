# NovaVita'nın ZSM-Q+ benzeri proje değerlendirme simülasyonu
projeler = {
"robotik_protez_gelistirme": {
"potansiyel_katki_zep": 8.0, # 0-10 arası
"toplumsal_fayda": 9.5, # 0-10 arası (erişilebilirlik,
yaşam kalitesi)
"etik_uyum_swordgeist": 0.95, # 0-1 arası
"surdurulebilirlik": 0.8 # 0-1 arası (malzeme, enerji)
},
"luks_kozmetik_implant_modulu": {
"potansiyel_katki_zep": 6.0,
"toplumsal_fayda": 2.0, # Düşük (estetik kaygı, eşitsizlik
potansiyeli)
"etik_uyum_swordgeist": 0.60, # Sınırda (gereklilik, yan

etkiler?)
"surdurulebilirlik": 0.5
},
"acil_durum_hizli_tani_cihazi": {
"potansiyel_katki_zep": 7.0,
"toplumsal_fayda": 9.8, # Çok yüksek (hayat kurtarma
potansiyeli)
"etik_uyum_swordgeist": 0.98, # Yüksek (erişim adaleti önemli)
"surdurulebilirlik": 0.75
}
}
# Önceliklendirme Ağırlıkları (Şirket stratejisine göre ayarlanabilir)
agirliklar = {
"katki": 0.20,
"fayda": 0.40, # Toplumsal fayda en önemli
"etik": 0.30, # Etik uyum çok önemli
"surdurulebilirlik": 0.10
}
def zsm_q_plus_degerlendir(proje_adi, proje_verileri):
"""Proje verilerine göre ZSM-Q+ benzeri bir öncelik skoru
hesaplar."""
skor = (proje_verileri["potansiyel_katki_zep"] / 10 *
agirliklar["katki"] +
proje_verileri["toplumsal_fayda"] / 10 *
agirliklar["fayda"] +
proje_verileri["etik_uyum_swordgeist"] * agirliklar["etik"]
+
proje_verileri["surdurulebilirlik"] *
agirliklar["surdurulebilirlik"])
return round(skor * 100, 2) # Skoru 100 üzerinden ifade et
# Projeleri değerlendir ve sırala
proje_skorlari = {}
for ad, puanlar in projeler.items():
proje_skorlari[ad] = zsm_q_plus_degerlendir(ad, puanlar)
# Skorlara göre sıralanmış projeler
sirali_projeler = sorted(proje_skorlari.items(), key=lambda item:
item[1], reverse=True)
print("Zulficore İlkelerine Göre Proje Önceliklendirme Skorları:")
for proje, skor in sirali_projeler:
print(f"- {proje.replace('_', ' ').title()}: {skor}")
