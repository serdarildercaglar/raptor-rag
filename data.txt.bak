import pandas as pd
import matplotlib.pyplot as plt
import numpy as np
# Başlangıç (Yıl 0 - 2025) ve sonraki 5 yıl
years = list(range(2025, 2031))
population = [10000] # Başlangıç nüfusu
unemployment_rate = [22.0] # Başlangıç işsizlik oranı (%)
happiness_index = [4.3] # Başlangıç mutluluk endeksi (1-10)
ethics_score = [58.0] # Başlangıç etik skor (0-100)

innovation_local = [2] # Başlangıç yerel inovasyon/proje sayısı
migration_rate = [-1.5] # Başlangıç net göç oranı (%) - Göç veriyor
# 5 Yıllık Simülasyon (İyimser Zulficore Etkisi)
for i in range(1, 6):
# Nüfus, göçün tersine dönmesiyle artar
pop_change = population[-1] * (migration_rate[-1] / 100)
population.append(int(population[-1] + pop_change +
np.random.randint(20,50))) # Doğal artış da eklenir
# İşsizlik azalır (yeni iş/katkı alanları)
unemployment_rate.append(max(3.0, unemployment_rate[-1] -
np.random.uniform(2.5, 4.0)))
# Mutluluk artar (katılım, adalet, anlam)
happiness_index.append(min(9.5, happiness_index[-1] +
np.random.uniform(0.7, 1.0)))
# Etik skor artar (eğitim, şeffaflık, onarım)
ethics_score.append(min(95.0, ethics_score[-1] +
np.random.uniform(5.0, 7.0)))
# İnovasyon artar (eğitim, işbirliği, ZEP teşviki)
innovation_local.append(innovation_local[-1] + np.random.randint(3,
8) * (i+1)) # Yıllarla artan ivme
# Göç tersine döner (fırsatlar artar)
migration_rate.append(min(2.5, migration_rate[-1] +
np.random.uniform(0.6, 1.0)))
# DataFrame oluşturma
df_kasaba = pd.DataFrame({
"Yıl": years,
"Nüfus (Tahmini)": population,
"İşsizlik Oranı (%)": [round(x, 1) for x in unemployment_rate],
"Mutluluk Endeksi (1-10)": [round(x, 1) for x in happiness_index],
"Etik Skor (0-100)": [round(x, 1) for x in ethics_score],
"Yerel İnovasyon/Proje Sayısı": innovation_local,
"Net Göç Oranı (%)": [round(x, 1) for x in migration_rate]
})
print("Yeni Işık Kasabası Zulficore Pilot Uygulama Simülasyonu (5
Yıl):")
